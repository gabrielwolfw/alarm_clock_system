#include "timer.h"

void init_timer(){ //Valores iniciales tanto del reloj como alarma
	current_hours = 0;
	current_minutes = 0;
	alarm_flag = 0;
	alarm_hours = 0;
	alarm_minutes = 0;
}

void set_time(int hours, int minutes){
	current_hours = hours % MAX_HOURS;
	current_minutes = minutes % MAX_MINUTES;
}
